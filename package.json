{
  "name": "koa-react-boiler",
  "version": "1.0.0",
  "description": "An experimental SSR application using React and Koa",
  "main": "index.js",
  "repository": "github:nathanhumphrey/koa-react-boiler",
  "private": true,
  "engines": {
    "node": ">= 10.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "scripts": {
    "test": "npx jest",
    "test:v": "npx jest --verbose",
    "open": "npx open http://localhost:3000",
    "dev": "npm run open && NODE_ENV=\"development\" npx babel-node ./src/server/index.js",
    "build": "NODE_ENV=production npm run build:webpack && npm run build:lib",
    "build:webpack": "webpack",
    "build:lib": "babel -d lib src",
    "start": "NODE_ENV=production node lib/server/index.js"
  },
  "keywords": [
    "ssr",
    "koa",
    "react",
    "isomorphic"
  ],
  "author": {
    "name": "Nathan Humphrey",
    "email": "nathpaulhump@gmail.com",
    "url": "https://nathanhumphrey.ca"
  },
  "license": "MIT",
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/public/"
    ],
    "testRegex": "\\.test\\.js",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@babel/preset-env": "^7.8.0",
    "@babel/register": "^7.8.0",
    "@hot-loader/react-dom": "^16.11.0",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.11.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-webpack": "^5.2.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.8.0",
    "@babel/node": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-react": "^7.8.0",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@types/react": "^16.9.18",
    "autoprefixer": "^9.7.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "clean-css-loader": "^2.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.2",
    "open": "^7.0.0",
    "open-cli": "^5.0.0",
    "postcss": "^6.0.23",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
